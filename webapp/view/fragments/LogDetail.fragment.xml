                    id="overflowToolbarSpacer"
                    width="40%"
                />
                <Label
                    id="LocalCurrencyLabel"
                    text="로컬통화 금액 합계 :"
                    class="sapUiTinyMargin"
                />
                <Input
                    id="LocalCurrencyInput"
                    value="{parts: ['headerModel>/Hslt', 'headerModel>/Rhcur'],  type:'sap.ui.model.type.Currency',   formatOptions: {showMeasure: false}}"
                    valueState="{path: 'headerModel>/Hslt', formatter: '.formatter.InputValueState'}"
                    change="bindingInputData"
                    width="10%"
                    blocked="true"
                />

                <Label
                    id="GroupCurrencyLabel"
                    text="그룹통화 금액 합계 :"
                    class="sapUiTinyMargin"
                />

                <Input
                    id="GroupCurrencyInput"
