                    valueState="{path: 'headerModel>/Kslt', formatter: '.formatter.InputValueState'}"
                    width="10%"
                    change="bindingInputData"
                    blocked="true"
                />

                <ToolbarSpacer id="overflowToolbarSpacer2" />
                <Button
                    id="pgdc0020_1.smarttable.btn.currenycheck"
                    enabled="true"
                    press="onToolbarCurrencyCheckPress"
                    text="{i18n>CurrencyCheckButtonText}"
                    tooltip="{i18n>CurrencyCheckButtonText}"
                />
                <Button
                    id="pgdc0020_1.smarttable.btn.accountcheck"
                    enabled="{ui>/checkBtnEnabled}"
                    press="onToolbarAccountCheckPress"
                    text="{i18n>AccountCheckButtonText}"
                    tooltip="{i18n>AccountCheckButtonText}"
                />
                <Button
                    id="pgdc0020_1.smarttable.btn.accountmapping"
                    enabled="{ui>/deleteBtnEnabled}"
